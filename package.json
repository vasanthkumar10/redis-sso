    "access": "public"
  },
  "keywords": [
    "sso",
    "redis",
    "node",
    "module",
    "cache",
    "js",
    "javascript",
    "node-redis"
  ],
  "imports": {
    "#*": "./*"
  },
  "engines": {
    "node": ">=18.16.0"
  }
