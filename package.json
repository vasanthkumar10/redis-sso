{
  "name": "redis-sso",
  "version": "1.0.1",
  "description": "SSO handshake",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "compile": "tsc",
    "generate:docs": "npx compodoc -p tsconfig.json",
    "lint": "eslint . || echo 'Run \"npm run lint:fix\" to automatically fix issues.'",
    "lint:fix": "eslint --fix .",
    "prepare": "husky",
    "postversion": "git push && git push --tags && npm run publish:pkg",
    "publish:pkg": "npm publish"
  },
  "dependencies": {
    "@am92/redis": "^2.0.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-methods": "^7.25.7",
    "@compodoc/compodoc": "^1.1.26",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.7.8",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-n": "^17.11.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "contributors": [
    {
      "name": "Vasanthkumar V",
      "email": "vasizebron10@gmail.com",
      "url": "https://github.com/vasanthkumar10"
    }
  ],
  "license": "MIT",
